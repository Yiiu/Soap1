{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single", "jsx-double"],
        "no-console": [false, "log", "error"],
        "semicolon": [false, "always"],
        "trailing-comma": [false, {"multiline": "always", "singleline": "never"}]
    },
    "rulesDirectory": []
}